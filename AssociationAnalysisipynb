    "import matplotlib.pyplot as plt\n",
    "plt.scatter(confidence, num_rules)\n",
    "plt.plot(confidence, num_rules)\n",
    "plt.ylabel('Number of Rules Generated')\n",
    "plt.xlabel('Confidence Treshold')\n",
    "plt.show()"
   ]
